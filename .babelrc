{
  "plugins": [
    "transform-class-properties",
    "transform-export-extensions",
    "transform-object-rest-spread",
    "transform-strict-mode"
  ],
  "presets": ["flow"],
  "env": {
    "development": {
      "presets": [
        ["env", {
          "targets": {
            "node": "7.10.1"
          }
        }]
      ]
    },
    "production": {
      "plugins": [
        "external-helpers",
        "transform-class-properties"
      ],
      "presets": [
        ["env", {
          "targets": {
            "node": "7.10.1"
          },
          "modules": false
        }],
        "minify"
      ],
    },
    "test": {
      "plugins": [
        "istanbul",
        "transform-class-properties"
      ],
      "presets": [
        ["env", {
          "targets": {
            "node": "7.10.1"
          }
        }]
      ]
    }
  }
}
